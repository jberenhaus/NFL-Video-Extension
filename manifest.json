{
  "manifest_version": 0,

  "name": "NFL Video Fixer",
  "description": "This extension ",
  "version": "0.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
	"http://*.baltimoreravens.com/*",
	"http://*.nfl.com/*"
  ]
}
